<div class="container mt-32">
  <div class="row justify-content-center">
    <div class="col-lg-6">

     <%= simple_form_for [@car, @booking] do |f|%>
        <%= f.input :date_begin, as: :date, html5: true %>
        <%= f.input :date_end, as: :date, html5: true %>
        <%= link_to "Retour", car_path(@car), class: "btn-flat"  %>
        <%= f.button :submit, class: "btn-flat" %>
      <% end %>
    </div>
  </div>
</div>
